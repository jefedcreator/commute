{
  "name": "commute",
  "version": "1.0.0",
  "description": "Commute Api",
  "main": "index.ts",
  "scripts": {
    "dev": "nodemon",
    "start": "tsx -r module-alias/register -r dotenv/config ./dist/src",
    "build": "rm -rf dist/src && tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "build-secrets": "tsx -r module-alias/register ./dist/config/build.env.js",
    "start:dev": "yarn run build && tsx -r module-alias/register ./dist",
    "test": "set NODE_ENV=test && tsx --test --test-concurrency=1 test/**test.ts"
  },
  "repository": "https://github.com/jefedcreator/commute",
  "author": "Commute",
  "license": "MIT",
  "private": true,
  "_moduleAliases": {
    "@controllers": "dist/src/controllers",
    "@middlewares": "dist/src/middlewares",
    "@validators": "dist/src/validators",
    "@services": "dist/src/services",
    "@models": "dist/src/models",
    "@utils": "dist/src/utils",
    "@config": "dist/src/config",
    "@types": "dist/src/types"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "date-fns": "^3.6.0",
    "envalid": "^8.0.0",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "gitops-secrets": "^0.0.6",
    "helmet": "^7.0.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mailgun.js": "^9.3.0",
    "mocha": "^10.4.0",
    "module-alias": "^2.2.3",
    "mongoose": "^7.6.2",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "2",
    "nodemailer": "^6.9.6",
    "otplib": "^12.0.0-3",
    "reflect-metadata": "^0.2.2",
    "tsx": "^4.7.2",
    "typedi": "^0.10.0",
    "typescript": "^5.3.3",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.3.14",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.19",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/multer": "^1.4.9",
    "@types/node": "^20.8.6",
    "@types/node-fetch": "^2.6.7",
    "@types/nodemailer": "^6.4.13",
    "@types/supertest": "^6.0.2",
    "chai": "^5.1.0",
    "dotenv": "^16.3.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "supertest": "^6.3.4"
  }
}
